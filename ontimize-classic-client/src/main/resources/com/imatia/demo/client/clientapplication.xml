<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- File to describe the application modules-->
<MainApplication
   title="Ontimize Archetype"
   logindialogclass="com.ontimize.login.ShapeLoginDialog"
   splash="com/imatia/demo/client/images/00_login-splash.png;2"
   loginicon="com/imatia/demo/client/images/back_splash.png"
   icon="com/imatia/demo/client/images/ontimize16x16.png"
   preferences="OntimizeArchetype.conf"
   logintext="session_start"
   resources="com.imatia.demo.client.i18n.bundle"
   locale="es_ES"
   encrypt="no">
   <!--  Application configuration params  -->
   <ReferenceLocator remotelocatorname="ServerName"
       local="false"
       class="com.ontimize.locator.ReferenceLocator"
       packageorhostname="localhost"
       port="49015"
       checkservermessagesperiod="4000"
   />
   <!-- Menu and toolbar configuration -->
    <Menu archive="menu/menu.xml" />
    <MenuListener class="com.imatia.demo.client.menu.AppMenuListener" />
    <Toolbar archive="toolbar/toolbar.xml"/>
    <ToolbarListener class="com.ontimize.gui.DefaultToolBarListener" />
    <FormManager id="Managerabout" form="modules/administration/formabout.xml" imanager="com.ontimize.gui.BasicInteractionManager" />
    <FormManager id="Managerusers" form="modules/administration/formusers.xml" imanager="com.ontimize.gui.BasicInteractionManager" tree="modules/administration/treemanagerusers.xml" />
    <FormManager id="managersqlqueries" form="modules/sql/formSQLQueries.xml" delayedload="yes" imanager="com.imatia.demo.client.modules.sql.IFMSQLQueries" />
</MainApplication>